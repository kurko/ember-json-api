    "broccoli-es6-concatenator": "^0.1.4",
    "broccoli-es6-transpiler": "^0.1.0",
    "broccoli-merge-trees": "^0.1.3",
    "broccoli-static-compiler": "^0.1.4",
    "broccoli-template": "^0.1.0",
