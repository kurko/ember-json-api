    "test": "npm run build-test && phantomjs test_build/tests/runner.js test_build/tests/index.html && rm -rf test_build",
    "test-server": "./node_modules/.bin/broccoli serve",
    "serve": "./node_modules/.bin/broccoli serve"
  },
  "devDependencies": {
    "bower": "",
